{
  "name": "open-data-explorer",
  "version": "1.0.0-alpha.4",
  "description": "React based application to explore open datasets interactively.",
  "repository": "https://github.com/DataSF/open-data-explorer",
  "license": "MIT",
  "dependencies": {
    "airbrake-js": "^0.9.1",
    "bootstrap-3-card": "^0.2.0",
    "bootstrap-sass": "^3.3.7",
    "d3": "^3.5.17",
    "fixed-data-table": "^0.6.0",
    "lodash": "^4.6.1",
    "moment": "^2.13.0",
    "pluralize": "^2.0.0",
    "prop-types": "^15.5.10",
    "qs": "^6.4.0",
    "rc-slider": "^3.5.1",
    "react": "^15.5.4",
    "react-addons-update": "^15.1.0",
    "react-autosuggest": "^9.0.0",
    "react-bootstrap": "^0.31.0",
    "react-bootstrap-card": "^0.1.5",
    "react-bootstrap-daterangepicker": "^3.2.2",
    "react-copy-to-clipboard": "^4.2.3",
    "react-date-range": "^0.9.4",
    "react-dimensions": "^1.2.0",
    "react-dom": "^15.5.4",
    "react-ga": "^2.2.0",
    "react-instantsearch": "4.0.0",
    "react-radio-group": "^2.2.0",
    "react-redux": "^4.4.1",
    "react-router": "3.0.5",
    "react-router-redux": "^4.0.1",
    "react-select": "1.0.0-rc.5",
    "react-show-more": "^1.1.1",
    "react-virtualized": "^9.9.0",
    "recharts": "^0.18.0",
    "redux": "^3.3.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "soda-js": "^0.2.2",
    "titleize": "^1.0.0",
    "underscore.string": "^3.3.4"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "autoprefixer": "7.1.0",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-preset-react-app": "^3.0.0",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "css-loader": "0.28.1",
    "dotenv": "4.0.0",
    "es6-symbol": "^3.1.1",
    "eslint": "3.19.0",
    "eslint-config-react-app": "^1.0.4",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.33.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-jsx-control-statements": "^2.1.1",
    "eslint-plugin-react": "7.0.1",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.28.0",
    "jest": "20.0.3",
    "jsx-control-statements": "^3.1.5",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.5",
    "promise": "7.1.1",
    "react-dev-utils": "^3.0.0",
    "react-error-overlay": "^1.0.7",
    "style-loader": "0.17.0",
    "sw-precache-webpack-plugin": "0.9.1",
    "url-loader": "0.5.8",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.4.5",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive",
    "start-js": "node scripts/start.js",
    "start": "npm-run-all -p watch-css start-js",
    "build": "REACT_APP_CONTEXT=${REACT_APP_CONTEXT} npm run build-css && node scripts/build.js",
    "build:staging": "REACT_APP_CONTEXT=staging npm run build",
    "build:production": "REACT_APP_CONTEXT=production npm run build",
    "test": "node scripts/test.js --env=jsdom",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "jsx-control-statements"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "react/jsx-no-undef": [
        "error",
        {
          "allowGlobals": true
        }
      ]
    },
    "globals": {
      "Choose": true,
      "When": true,
      "Otherwise": true
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  }
}
